		"@sveltejs/kit": "^1.5.0",
		"@types/markdown-it": "^12.2.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-headless-table": "^0.17.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@types/node": "^18.15.0",
		"csvtojson": "^2.0.10",
		"gray-matter": "^4.0.3",
		"markdown-it": "^13.0.1"
	}
