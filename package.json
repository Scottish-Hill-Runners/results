{
	"name": "scottish-hill-runners",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean": "rm -r src/routes/RA* src/routes/races",
		"dev": "tsc && node src/pre-build.js && vite dev",
		"build": "tsc && node src/pre-build.js && vite build",
